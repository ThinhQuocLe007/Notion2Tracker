create table if not exists notion_data(
    id int auto_increment primary key, 
    Name varchar(255), 
    Date DATE, 
    Progression FLOAT, 
    Formula FLOAT, 
    DATA_TIME FLOAT, 
    ENGLISH_TIME FLOAT, 
    OTHER_TIME FLOAT, 
    RELAX_TIME FLOAT, 
    Walking FLOAT, 
    NoFap BOOLEAN, 
    UNIQUE(Name, Date)
) ; 